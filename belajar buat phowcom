    "                                                # x= 0.5 y= -0.05\n",
    "# tipe keempat\n",
    "\n",
    "plt.figure(3) #figure1 adalah contianer semuanya\n",
    "ax = plt.subplot(111) # mau membuat posisinya di luar\n",
    "plt.plot(t1,y1, label=\"sin(0)\") \n",
    "plt.plot(t2,y2, label=\"sin(90)\")\n",
    "\n",
    "box = ax.get_position() #si box ngambil ax dan langsung mengambil posisinya\n",
    "ax.set_position([box.x0, box.y0, box.width*0.7, box.height]) # menggeser posisinya kekiri box.xo dan box.yo adl posisi plotnya, widthnya adlaah lebarnya jadi kita perkecil 0.7 atau 7 persen, lalu height untuk tingginya \n",
    "plt.legend(loc=\"upper right\", bbox_to_anchor=(1.5,1)) \n",
    "\n",
    "# jadi intinya subplot ngambil data yang ada lalu diperkecil dengna teknik bibox\n",
    "#tamplikan\n",
    "plt.show()\n"
