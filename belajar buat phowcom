  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x288 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "# membuat label dan title\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "#buat data\n",
    "def sinusGenerator(amplitudo,frekuensi,tAkhir, theta):\n",
    "    t = np.arange(0,tAkhir,0.1)\n",
    "    y = amplitudo * np.sin(2*frekuensi*t + np.deg2rad(theta)) \n",
    "    return t,y\n",
    "\n",
    "#kita set dulu\n",
    "amplitudo = 1 \n",
    "frekuensi = 1\n",
    "theta =  0\n",
    "tAkhir = 4\n",
    "\n",
    "t,y = sinusGenerator(amplitudo,frekuensi,tAkhir,theta)\n",
    "\n",
    "#buat plot\n",
    "\n",
    "judul = 'Grafik Sinusoidal\\n'\n",
    "rumus = r'$ \\mathcal{Y} = A.sin(2 \\omega t + \\theta)$' + '\\n' \n",
    "parameter1 = r'$ A = $' + str(amplitudo) + ' cm,  ' tchal itu untuk simbol , klau tidak makai mathcal akan biasa\n",
    "parameter2 = r'$ \\omega = $' + str(frekuensi) + r'$ \\mathit{Hz}$' +',   ' #frekuensi tuh ka hertz jadi dia italic(it)\n",
    "parameter3 = r'$ \\theta = $' + str(theta) + r'$^{o} $' #ngeluarin fasa/ derajat sudut\n",
    "                #theta simbol     \n",
    "\n",
    "plt.plot(t,y)\n",
    "\n",
    "plt.title(judul + rumus + parameter1 + parameter2 + parameter3) # judul dan rumus\n",
    "\n",
    "plt.xlabel('waktu(detik)') #label\n",
    "plt.ylabel('magnituda(cm)')  #label\n",
    "\n",
    "#tampilan\n",
    "\n",
    "plt.show()\n",
    "\n",
    "\n"
   ]
