    "# membuat legend (menambah label, title, legend )\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "#buat data\n",
    "def sinusGenerator(amplitudo,frekuensi,tAkhir, theta):\n",
    "    t = np.arange(0,tAkhir,0.1)\n",
    "    y = amplitudo * np.sin(2*frekuensi*t + np.deg2rad(theta)) \n",
    "    return t,y\n",
    "t1,y1 = sinusGenerator(1,1,4,0)\n",
    "t2,y2  = sinusGenerator(1,1,4,90)\n",
    "#membuat plot dan menabahkan legend = memberitahukan grafik ini apa\n",
    "\n",
    "# tipe pertama\n",
    "#plt.plot(t1,y1, label=\"sin(0)\")#kasih keyword label dengan fungsi yang diinginkan\n",
