    "# menambahkan text dimana saja -> gimana dia pick nya itu pas diantara titik2 yagn penting\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "#buat data\n",
    "def sinusGenerator(amplitudo,frekuensi,tAkhir, theta):\n",
    "    t = np.arange(0,tAkhir,0.1)\n",
    "    y = amplitudo * np.sin(2*frekuensi*t + np.deg2rad(theta)) \n",
    "    return t,y\n",
    "t,y = sinusGenerator(1,1,4,0)\n",
    "\n",
    "plt.plot(t,y)\n",
    "#plt.text(2,1, 'sin(0)') #posisi x dan posisi y\n",
    "#plt.text(2,0.5, 'sin(30)')\n",
    "plt.text(2,0.5, r'$ y = A.sin(2 \\omega t)  $' ) #tambahkan regualr expression\n",
    "plt.text(2,0.4, r'$ y = \\mathcal{A} = 1 cm, \\omega = 1 hz  $' ) #tambahkan regualr expression\n",
